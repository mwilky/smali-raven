<?xml version="1.0" encoding="utf-8"?>
<ripple android:color="@color/overlay_button_ripple"
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:androidprv="http://schemas.android.com/apk/prv/res/android">
    <item android:id="@android:id/background">
        <inset android:insetTop="4.0dip" android:insetBottom="4.0dip">
            <shape android:shape="rectangle">
                <solid android:color="?android:^attr-private/colorAccentPrimary" />
                <corners android:radius="20.0dip" />
                <size android:height="40.0dip" />
            </shape>
        </inset>
    </item>
    <item android:id="@android:id/mask">
        <inset android:insetTop="4.0dip" android:insetBottom="4.0dip">
            <shape android:shape="rectangle">
                <solid android:color="?android:textColorPrimary" />
                <corners android:radius="20.0dip" />
                <size android:height="40.0dip" />
            </shape>
        </inset>
    </item>
</ripple>